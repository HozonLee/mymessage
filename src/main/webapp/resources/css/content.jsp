	width:200px;
}

a:link {
	color: #000;
	text-decoration: none;
}		/* 未访问的链接 */
a:visited {
	color: #000;
	text-decoration: none;
	}	/* 已访问的链接 */
a:hover {
	color: blue; 
	text-decoration: underline;
}		/* 鼠标移动到链接上，须在link，visited后 */
a:active {
	color: #000; 
	text-decoration: none;
}
